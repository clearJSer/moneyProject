<div class="m-register">
	<div id="header"></div>
	<section>
		<div class="mui-content">
			<div class="mg_logo">
		    	<img src="images/img-register/logo.png">
		    </div>
		    <div class="login_con">
		    	<div class="login_input_group">
					<label for="mobile">手机号码：</label>
					<input type="text" class="clear_input" placeholder="请输入手机号" id="mobile" name="mobile">
	     	  	</div>
	     	  	
	     	  	<div class="verification_input">
					<label for="verificationCode">验证码：</label>
					<input type="text" class="verifica_input" id="verificationCode">
					<p class="verifica_img"><img src="http://www.dfhfax.com/dhapi/services/sm/getVerifyCode"></p>
					<a href="javascript:;" class="verifica_a">看不清楚，换一张</a>
				</div>
				<div class="login_next_con">
		     		<div class="login_input_group login_input_group_small">
			     	    <div class="login_input_group_s_w">
